extends layout

block content
  .ui-wrapper
    .ui-header
      .cash
        p
          span#money 0
          span.currency  $
      #time
        p 1.1.1970 00:00
    .ui-content
      .left
        canvas(width="640", height="480")#drawable-canvas
          | Oops! Your browser is too OLD to play games with it - no stupid business for you.
      .right
        table.status-area
          thead
              tr
                th.no-bg
                  p Project ongoing:&nbsp;
                    span#project No project...
        table.buy-area
          tbody
            tr
              td#coder.profession
                p
                  span Coder &raquo; &nbsp;
                  span.price 0
                  span.currency  $
              td#cleaner.profession
                p
                  span Cleaner &raquo; &nbsp;
                  span.price 0
                  span.currency  $
            tr
              td#cook.profession
                p
                  span Cook &raquo; &nbsp;
                  span.price 0
                  span.currency  $
              td#manager.profession
                p
                  span Manager &raquo; &nbsp;
                  span.price 0
                  span.currency  $
            tr
              td(colspan="2", id="buy-floor")
                p
                  span Buy new floor to office &raquo; &nbsp;
                  span.price 0
                  span.currency  $
        table.person-area
          tbody
            tr
              td
                p Slap
              td
                p Slap
              td
                p Slap
            tr
              td
                p Slap
              td.no-bg#person
              td
                p Slap
            tr
              td
                p Slap
              td
                p Slap
              td
                p Slap

